
stage('Checkout') {
 steps {
  git credentialsId: 'aemad1718', url: 'https://github.com/aemad1718/SimpleCoreApp', branch: 'master'
 }
}
stage('Restore Packages') {
 steps {
  bat "dotnet restore"
 }
}

stage('Clean') {
 steps {
  bat "dotnet clean"
 }
}

stage('Build') {
 steps {
  bat "dotnet build --configuration Release"
 }
}
stage('Build') {
 steps {
  bat "dotnet pack --no-build --output nupkgs"
 }
}
